<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="tapestry5-high* test suite" annotations="1.5">
    <test name="Integration Tests"  enabled="true">
        <parameter name="tapestry.web-app-folder" value="src/test/webapp"/>
        <parameter name="tapestry.browser-start-command" value="*firefox" />

        <classes>
            <class name="be.eliwan.tapestry5.high.util.JsonUtilTest"></class>
            <class name="be.eliwan.tapestry5.high.test.HighchartsTest"></class>
            <class name="be.eliwan.tapestry5.high.test.HighstockTest"></class>
        </classes>
    </test>
</suite>
